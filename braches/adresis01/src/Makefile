MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-dlib \
		sub-server \
		sub-client

first: all

all: Makefile $(SUBTARGETS)

dlib/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "dlib" || $(MKDIR) "dlib"
	cd dlib && $(QMAKE) dlib.pro -o $(MAKEFILE)
sub-dlib: dlib/$(MAKEFILE) FORCE
	cd dlib && $(MAKE) -f $(MAKEFILE)

server/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "server" || $(MKDIR) "server"
	cd server && $(QMAKE) server.pro -o $(MAKEFILE)
sub-server: server/$(MAKEFILE) FORCE
	cd server && $(MAKE) -f $(MAKEFILE)

client/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "client" || $(MKDIR) "client"
	cd client && $(QMAKE) client.pro -o $(MAKEFILE)
sub-client: client/$(MAKEFILE) FORCE
	cd client && $(MAKE) -f $(MAKEFILE)

Makefile: src.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile src.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile src.pro

all: $(SUBTARGETS)
qmake_all: dlib/$(MAKEFILE) server/$(MAKEFILE) client/$(MAKEFILE)
	 ( [ -d dlib ] && cd dlib ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d server ] && cd server ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d client ] && cd client ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d dlib ] && cd dlib ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d server ] && cd server ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d client ] && cd client ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d dlib ] && cd dlib ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d server ] && cd server ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d client ] && cd client ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d dlib ] && cd dlib ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d server ] && cd server ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d client ] && cd client ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d dlib ] && cd dlib ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d server ] && cd server ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d client ] && cd client ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs


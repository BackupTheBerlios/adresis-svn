#############################################################################
# Makefile for building: 3rdparty
# Generated by qmake (1.07a) (Qt 3.3.6) on: Tue Sep 12 18:58:20 2006
# Project:  3rdparty.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile 3rdparty.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-psql

first: all

all: Makefile $(SUBTARGETS)

psql/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "psql" || $(MKDIR) "psql"
	cd psql && $(QMAKE) psql.pro -o $(MAKEFILE)
sub-psql: psql/$(MAKEFILE) FORCE
	cd psql && $(MAKE) -f $(MAKEFILE)

Makefile: 3rdparty.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile 3rdparty.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile 3rdparty.pro

all: $(SUBTARGETS)
qmake_all: psql/$(MAKEFILE)
	 ( [ -d psql ] && cd psql ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d psql ] && cd psql ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d psql ] && cd psql ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d psql ] && cd psql ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d psql ] && cd psql ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

